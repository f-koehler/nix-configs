{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^tinymediamanager.nix$", "^unifi.nix$"],
      "matchStrings": [
        "image\\s*=\\s*\\\"(?<depName>.+)\\:(?<currentValue>.+)\\\""
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "docker"
    }
  ]
}
